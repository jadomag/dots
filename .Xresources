! ------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:ee282828, #ebdbb2, #ebdbb2
rofi.color-normal: argb:0000000, #ebdbb2, argb:0000000, #ebdbb2, #282828
rofi.color-active: argb:0000000, #458588, argb:0000000, #458588, #282828
rofi.color-urgent: argb:0000000, #cc241d, argb:0000000, #cc241d, #282828
rofi.separator-style: solid

! ------------------------------------------------------------
! urxvt config
! ------------------------------------------------------------
! set font
URxvt.font: 		xft:Roboto Mono:pixelsize=14:antialias=true

! 'fake' transparency
! URxvt.transparent: 	true
! URxvt.shading:		10

! 'real' transparency
urxvt.depth: 32
urxvt.background: rgba:0000/0000/0000/dddd

URxvt.scrollBar:	false

