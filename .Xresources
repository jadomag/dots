! ------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:ee282828, #ebdbb2, #ebdbb2
rofi.color-normal: argb:0000000, #ebdbb2, argb:0000000, #ebdbb2, #282828
rofi.color-active: argb:0000000, #458588, argb:0000000, #458588, #282828
rofi.color-urgent: argb:0000000, #cc241d, argb:0000000, #cc241d, #282828
rofi.separator-style: solid

! ------------------------------------------------------------
! urxvt config
! ------------------------------------------------------------
! set font
URxvt.font: 		xft:Roboto Mono:pixelsize=14:antialias=true

! 'fake' transparency
URxvt.transparent: 	true
URxvt.shading:		10

! 'real' transparency
! urxvt.depth: 32
! urxvt.background: rgba:0000/0000/0000/dddd

URxvt.scrollBar:	false

! ------------------------------------------------------------
! color scheme
! ------------------------------------------------------------
! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2
