! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:ee202020, #cccccc, #cccccc
rofi.color-normal: argb:0000000, #cccccc, argb:0000000, #cccccc, #333333
rofi.color-active: argb:0000000, #6699cc, argb:0000000, #6699cc, #333333
rofi.color-urgent: argb:0000000, #f95656, argb:0000000, #f95656, #333333
rofi.separator-style: solid

! ------------------------------------------------------------
! urxvt config
! ------------------------------------------------------------

! set font
URxvt.font: 		xft:Roboto Mono:pixelsize=14:antialias=true

!  transparency
URxvt.transparent: 	true
URxvt.shading:		10

URxvt.scrollBar:	false

